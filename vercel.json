{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "clients/mobile/pubspec.yaml",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "cd clients/mobile && flutter pub get && flutter build web",
        "distDir": "clients/mobile/build/web"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}